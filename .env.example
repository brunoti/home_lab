# Home Lab Environment Variables
# Copy this file to .env and fill in your own values
# Generated: 2026-01-10

# ========================================
# JELLYFIN - Media Server
# ========================================
JELLYFIN_UID=1000
JELLYFIN_GID=1000
JELLYFIN_CACHE_DIR=/var/cache/jellyfin
JELLYFIN_CONFIG_DIR=/etc/jellyfin
JELLYFIN_LOG_DIR=/var/log/jellyfin
JELLYFIN_DATA_DIR=/var/lib/jellyfin

# ========================================
# KOEL - Music Streaming
# ========================================
KOEL_APP_KEY=
KOEL_APP_URL=http://koel.local
KOEL_ADMIN_EMAIL=admin@example.com
KOEL_ADMIN_PASSWORD=
KOEL_DB_HOST=postgres
KOEL_DB_PORT=5432
KOEL_DB_NAME=koel
KOEL_DB_USER=koel
KOEL_DB_PASSWORD=

# ========================================
# NAVIDROME - Music Server
# ========================================
NAVIDROME_PORT=4533
NAVIDROME_DATA_FOLDER=/data
NAVIDROME_MUSIC_FOLDER=/music
NAVIDROME_SCAN_INTERVAL=1m
NAVIDROME_BASEURL=http://navidrome.local

# ========================================
# CALIBRE - E-Book Manager
# ========================================
CALIBRE_UID=1000
CALIBRE_GID=1000
CALIBRE_CONFIG_DIR=/config
CALIBRE_LIBRARY_PATH=/books
CALIBRE_PORT=8080

# ========================================
# LAZYLIBRARIAN - Ebook Downloader
# ========================================
LAZYLIBRARIAN_UID=1000
LAZYLIBRARIAN_GID=1000
LAZYLIBRARIAN_CONFIG_DIR=/config
LAZYLIBRARIAN_LIBRARY_DIR=/books
LAZYLIBRARIAN_PORT=8383

# ========================================
# AFFINE - Open-Source Design Tool
# ========================================
AFFINE_PORT=3010
AFFINE_DATABASE_URL=postgresql://affine:password@postgres:5432/affine
AFFINE_REDIS_URL=redis://redis:6379
AFFINE_JWT_SECRET=

# ========================================
# HEADSCALE - Tailscale Control Server
# ========================================
HEADSCALE_UID=1000
HEADSCALE_GID=1000
HEADSCALE_CONFIG_DIR=/etc/headscale
HEADSCALE_DATA_DIR=/var/lib/headscale
HEADSCALE_PORT=8085
HEADSCALE_PRIVATE_KEY_PATH=/var/lib/headscale/private.key

# ========================================
# PIHOLE - DNS Sinkhole/DHCP Server
# ========================================
PIHOLE_UID=999
PIHOLE_GID=999
PIHOLE_DNS1=8.8.8.8
PIHOLE_DNS2=8.8.4.4
PIHOLE_WEB_PORT=80
PIHOLE_ADMIN_EMAIL=admin@example.com
PIHOLE_ADMIN_PASSWORD=
PIHOLE_TZ=UTC
PIHOLE_WEBPASSWORD=

# ========================================
# AUTHELIA - Authentication Server
# ========================================
AUTHELIA_JWT_SECRET=
AUTHELIA_SESSION_SECRET=
AUTHELIA_STORAGE_MYSQL_PASSWORD=
AUTHELIA_STORAGE_POSTGRES_PASSWORD=
AUTHELIA_STORAGE_POSTGRES_HOST=postgres
AUTHELIA_STORAGE_POSTGRES_PORT=5432
AUTHELIA_STORAGE_POSTGRES_DATABASE=authelia
AUTHELIA_STORAGE_POSTGRES_USERNAME=authelia
AUTHELIA_STORAGE_POSTGRES_SSLMODE=disable

# ========================================
# POSTGRESQL - Database Server
# ========================================
POSTGRES_USER=postgres
POSTGRES_PASSWORD=
POSTGRES_DB=postgres
POSTGRES_INITDB_ARGS=
PGDATA=/var/lib/postgresql/data/pgdata
POSTGRES_PORT=5432

# ========================================
# REDIS - In-Memory Data Store
# ========================================
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DBNUM=0
REDIS_APPENDONLY=yes

# ========================================
# PROMETHEUS - Metrics Collection
# ========================================
PROMETHEUS_PORT=9090
PROMETHEUS_RETENTION=15d
PROMETHEUS_STORAGE_PATH=/prometheus
PROMETHEUS_CONFIG_PATH=/etc/prometheus

# ========================================
# GRAFANA - Visualization & Monitoring
# ========================================
GRAFANA_PORT=3000
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=
GRAFANA_INSTALL_PLUGINS=
GRAFANA_SECURITY_ADMIN_USER=admin
GRAFANA_SECURITY_ADMIN_PASSWORD=
GF_DATABASE_TYPE=postgres
GF_DATABASE_HOST=postgres:5432
GF_DATABASE_NAME=grafana
GF_DATABASE_USER=grafana
GF_DATABASE_PASSWORD=

# ========================================
# LOKI - Log Aggregation
# ========================================
LOKI_PORT=3100
LOKI_CONFIG_PATH=/etc/loki
LOKI_STORAGE_PATH=/loki

# ========================================
# RCLONE - Cloud Storage Manager
# ========================================
RCLONE_CONFIG_DIR=/config/rclone
RCLONE_CACHE_DIR=/cache/rclone
RCLONE_LOG_LEVEL=INFO
RCLONE_SERVE_PORT=5572

# ========================================
# RADARR - Movie Management
# ========================================
RADARR_UID=1000
RADARR_GID=1000
RADARR_CONFIG_DIR=/config
RADARR_MOVIES_DIR=/movies
RADARR_PORT=7878
RADARR_API_KEY=

# ========================================
# SONARR - TV Series Management
# ========================================
SONARR_UID=1000
SONARR_GID=1000
SONARR_CONFIG_DIR=/config
SONARR_TV_DIR=/tv
SONARR_PORT=8989
SONARR_API_KEY=

# ========================================
# TRANSMISSION - BitTorrent Client
# ========================================
TRANSMISSION_UID=1000
TRANSMISSION_GID=1000
TRANSMISSION_CONFIG_DIR=/config
TRANSMISSION_DOWNLOAD_DIR=/downloads
TRANSMISSION_INCOMPLETE_DIR=/downloads/incomplete
TRANSMISSION_PORT=6881
TRANSMISSION_RPC_PORT=6969
TRANSMISSION_RPC_PASSWORD=

# ========================================
# PORTAINER - Docker Management UI
# ========================================
PORTAINER_PORT=9000
PORTAINER_EDGE_PORT=8000
PORTAINER_DATA_DIR=/data
PORTAINER_ADMIN_USER=admin
PORTAINER_ADMIN_PASSWORD=

# ========================================
# UPTIME KUMA - Uptime Monitoring
# ========================================
UPTIME_KUMA_PORT=3001
UPTIME_KUMA_DATA_DIR=/app/data
UPTIME_KUMA_STORAGE_TYPE=sqlite
UPTIME_KUMA_SQLITE_PATH=/app/data/kuma.db

# ========================================
# SPEEDTEST TRACKER - Internet Speed Monitor
# ========================================
SPEEDTEST_TRACKER_PORT=80
SPEEDTEST_TRACKER_DB_HOST=postgres
SPEEDTEST_TRACKER_DB_PORT=5432
SPEEDTEST_TRACKER_DB_NAME=speedtest
SPEEDTEST_TRACKER_DB_USER=speedtest
SPEEDTEST_TRACKER_DB_PASSWORD=
SPEEDTEST_TRACKER_LOG_CHANNEL=single
SPEEDTEST_TRACKER_APP_KEY=

# ========================================
# HOMEPAGE - Custom Dashboard
# ========================================
HOMEPAGE_UID=1000
HOMEPAGE_GID=1000
HOMEPAGE_CONFIG_DIR=/app/config
HOMEPAGE_PORT=3000

# ========================================
# NGINX - Web Server & Reverse Proxy
# ========================================
NGINX_PORT=80
NGINX_HTTPS_PORT=443
NGINX_CONFIG_DIR=/etc/nginx
NGINX_LOG_DIR=/var/log/nginx
NGINX_CACHE_DIR=/var/cache/nginx
NGINX_SSL_CERT_PATH=/etc/nginx/certs
NGINX_SSL_KEY_PATH=/etc/nginx/certs
